CALLBACK "count-lamins"
    EVERY ACCEPTED 1
    NODES 1
    WHERE BELONGS(X 0, BINDER LAMIN)
    COMPUTE SUM 1
CALLBACK "list-11"
    EVERY ACCEPTED 1
    NODES 2
    WHERE BELONGS(X 0, BINDER 1) AND BELONGS(X 1, BEAD_BINDING_TO 1)
    COMPUTE LIST DIST(X 0, X 1)
CALLBACK "prod-binders-beads"
    EVERY ACCEPTED 1
    NODES 2
    WHERE BELONGS(X 0, BINDER) AND BELONGS(X 1, BEAD)
    COMPUTE SUM 1
CALLBACK "sum-11"
    EVERY ACCEPTED 1
    NODES 2
    WHERE BELONGS(X 0, BINDER 1) AND BELONGS(X 1, BEAD_BINDING_TO 1)
    COMPUTE SUM DIST(X 0, X 1)
CALLBACK "score"
    EVERY ACCEPTED 1
    NODES 2
    WHERE DIST(X 0, X 1) == 1 AND BELONGS(X 0, BEAD)
    COMPUTE SUM ENERGY(X 0, X 1)
